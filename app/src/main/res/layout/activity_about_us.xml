<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/app_white"

    android:orientation="vertical">


    <include
        layout="@layout/layout_actionbar"
        android:layout_width="match_parent"
        android:layout_height="55dp"></include>

    <com.ghs.ghshome.custom.MineEditCustomView
        android:id="@+id/aboutUs_current_version_mcv"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:paddingLeft="25dp"
        android:paddingRight="25dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        app:left_button_text="当前版本"
        app:left_button_text_color="#000000"
        app:right_button_text_color="#FF9B9B9B"></com.ghs.ghshome.custom.MineEditCustomView>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="#FFF1F1F1"></View>

    <com.ghs.ghshome.custom.MineEditCustomView
        android:id="@+id/aboutUs_email_mcv"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:paddingLeft="25dp"
        android:paddingRight="25dp"
        app:left_button_text="邮箱"
        app:left_button_text_color="#000000"
        app:right_button_text="guangheplus@tehsingchiu.com"
        app:right_button_text_color="#FF9B9B9B"></com.ghs.ghshome.custom.MineEditCustomView>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="#FFF1F1F1"></View>

    <com.ghs.ghshome.custom.MineEditCustomView
        android:id="@+id/aboutUs_net_address_mcv"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:paddingLeft="25dp"
        android:paddingRight="25dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        app:left_button_text="官网"
        app:left_button_text_color="#000000"
        app:right_button_text="待定"
        app:right_button_text_color="#FF9B9B9B"></com.ghs.ghshome.custom.MineEditCustomView>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="#FFF1F1F1"></View>

    <com.ghs.ghshome.custom.MineEditCustomView
        android:id="@+id/aboutUs_public_no_mcv"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:paddingLeft="25dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:paddingRight="25dp"
        app:left_button_text="微信公众号"
        app:left_button_text_color="#000000"
        app:right_button_text="光合家社区"
        app:right_button_text_color="#FF9B9B9B"></com.ghs.ghshome.custom.MineEditCustomView>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="#FFF1F1F1"></View>

    <com.ghs.ghshome.custom.MineEditCustomView
        android:id="@+id/aboutUs_customer_service_mcv"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:paddingLeft="25dp"
        android:paddingRight="25dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        app:left_button_text="客服"
        app:left_button_text_color="#000000"
        app:right_button_text="59070887"
        app:right_button_text_color="#FF9B9B9B"></com.ghs.ghshome.custom.MineEditCustomView>


</LinearLayout>
