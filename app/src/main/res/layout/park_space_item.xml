<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="20dp"
    android:background="@drawable/sp_white"
    android:padding="15dp">

    <TextView
        android:id="@+id/park_space_name_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:text="车位名称"
        android:textColor="@color/seed_task_bg"
        android:textSize="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/park_space_type_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:text="绑定类型:"
        android:textColor="@color/gray_deeper"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@+id/park_space_name_tv" />

    <TextView
        android:id="@+id/park_space_effective_start_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingTop="2dp"
        android:paddingBottom="2dp"
        android:text="开始时间:"
        android:textColor="@color/gray_deeper"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@+id/park_space_type_tv" />

    <TextView
        android:id="@+id/park_space_effective_end_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:text="结束时间:"
        android:textColor="@color/gray_deeper"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@+id/park_space_effective_start_tv" />
</android.support.constraint.ConstraintLayout>